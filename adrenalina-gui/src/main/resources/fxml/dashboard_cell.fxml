<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.adrenalinici.adrenaline.gui.controller.DashboardCellController">

  <center>
    <HBox fx:id="players" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
      </padding>
    </HBox>
  </center>
  <top>
    <Label fx:id="position" style="-fx-background-color: WHITE;" BorderPane.alignment="CENTER">
      <font>
        <Font size="16.0"/>
      </font>
    </Label>
  </top>
  <opaqueInsets>
    <Insets/>
  </opaqueInsets>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>
  <bottom>
    <Label fx:id="content" style="-fx-background-color: WHITE;" BorderPane.alignment="CENTER">
      <font>
        <Font size="16.0"/>
      </font>
    </Label>
  </bottom>

</BorderPane>
